from flask import Flask, render_template, request, redirect, url_for
app = Flask(__name__)

@app.route("/app/home", methods=['GET', 'POST'])
def main():
    print(request.method)
    return render_template("webpage.html", name="admin")
    if request.method == "POST":
        print(request.form)
        return render_template("customer sign up page.html")
    else:
        return render_template("customer login page.html")

@app.route("/app/customer_login")
def customer_login():
    return render_template("customer login page.html")
@app.route("/app/admin_login")
def admin_login():
    return render_template("admin login page.html")

if __name__=="__main__":
    app.run(debug=True)
